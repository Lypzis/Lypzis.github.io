{
  "name": "lypzis.github.io",
  "version": "1.0.0",
  "description": "Lypzis personal portfolio webpage.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel devServer watch",
    "devServer": "live-server",
    "watch": "sass --watch ./src/sass/main.scss ./dist/css/style.css",
    "compile": "sass ./src/sass/main.scss ./dist/css/style.css",
    "prefix": "postcss --use autoprefixer -b 'last 10 versions' ./css/style.css -o ./css/style.css",
    "compress": "sass ./dist/css/style.css --style compressed",
    "build": "npm-run-all compile prefix compress"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.4.10",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "sass": "^1.14.3"
  }
}
